[
    {
        "title"     : "User Login", 
        "layout"    : "row", 
        "components":[
            { 
                "type"  : "container",
                "layout": "column",
                "cols"  : 10, 
                "components" : 
                [
                    {
                        "type"  : "text",
                        "name"  : "userid",
                        "placeholder"  : "Email id",
                        "validation": { "required": true },
                        "description":"Please enter the Emailid",
                        "class":"p-sm-1 px-xl-3"
                    },
                    {
                        "type"  : "password",
                        "name"  : "password",
                        "placeholder" : "Password",
                        "validation": { "required": true },
                        "description":"Please enter the Password",
                        "iconClass":"icon-58-password-cancel pwd-icon-position",
                        "functionname":"passwordview",
                        "class":"p-sm-1 px-xl-3",
                        "classname":"class-psw"
                    },
                    {
                        "type"  : "submit",
                        "label" : "Login",
                        "description":"Click here to login",
                        "class" : "col-md-12 text-center login-btn"
                    }
                ]
            }
        ]
    }
]